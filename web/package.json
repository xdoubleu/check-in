{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cpx ./serve.json ./out/ && serve out",
    "build": "yarn run swag && next build",
    "analyze": "cross-env ANALYZE=true next build",
    "format": "prettier --write './**/*.{ts,tsx}'",
    "lint": "eslint . && tsc --noEmit && jscpd . && knip && prettier --check './**/*.{ts,tsx}' && npmPkgJsonLint .",
    "lint:fix": "eslint . --fix",
    "swag": "npx openapi-typescript@5.4 ./docs/swagger.json -o ./src/api-wrapper/types/schema.d.ts --export-type",
    "test": "jest",
    "test:cov": "jest --coverage",
    "jest-preview": "jest-preview"
  },
  "dependencies": {
    "@mdx-js/loader": "3.1.1",
    "@mdx-js/react": "3.1.1",
    "@next/mdx": "15.5.4",
    "@popperjs/core": "2.11.8",
    "@sentry/nextjs": "10.17.0",
    "bootstrap": "5.3.8",
    "bootstrap-icons": "1.13.1",
    "cpx2": "8.0.0",
    "jest-preview": "0.3.2",
    "moment": "2.30.1",
    "moment-timezone": "0.6.0",
    "next": "15.5.4",
    "next-pwa": "5.6.0",
    "query-string": "9.3.1",
    "react": "19.1.1",
    "react-bootstrap": "2.10.10",
    "react-dom": "19.1.1",
    "react-hook-form": "7.64.0",
    "react-spinners": "0.17.0",
    "recharts": "3.2.1",
    "sass": "1.93.2",
    "serve": "14.2.5",
    "typescript": "5.9.2",
    "uglify-js": "3.19.3",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@next/bundle-analyzer": "15.5.4",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "30.0.0",
    "@types/node": "22.18.8",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "@types/uglify-js": "3",
    "@typescript-eslint/parser": "8.38.0",
    "cross-env": "10.1.0",
    "eslint": "9.37.0",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-redundant-undefined": "1.0.0",
    "eslint-plugin-sonarjs": "3.0.5",
    "eslint-plugin-testing-library": "7.11.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jest-mock": "30.2.0",
    "jest-websocket-mock": "2.5.0",
    "jscpd": "4.0.5",
    "knip": "5.64.1",
    "next-router-mock": "1.0.2",
    "npm-package-json-lint": "9.0.0",
    "openapi-typescript": "7.9",
    "prettier": "3.6.2",
    "ts-jest": "29.4.4",
    "typescript-eslint": "8.38.0"
  },
  "engines": {
    "node": "22.x",
    "yarn": "4.x"
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-alphabetical-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error",
      "prefer-alphabetical-devDependencies": "error"
    }
  },
  "jscpd": {
    "exitCode": 1,
    "pattern": "**/*.{ts,tsx}",
    "ignore": [
      "**/*.d.ts",
      "**/coverage/**",
      "**/node_modules/**",
      "**/*.test.{ts,tsx}"
    ],
    "format": [
      "typescript",
      "tsx"
    ]
  },
  "prettier": {
    "parser": "typescript",
    "semi": false,
    "trailingComma": "none"
  },
  "packageManager": "yarn@4.10.3"
}
